{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "OmniKee",
  "version": "0.1.23",
  "identifier": "de.semicolonsoftware.omnikee",
  "build": {
    "frontendDist": "../www/dist/spa",
    "devUrl": "http://localhost:9000",
    "beforeDevCommand": {
      "cwd": "../www",
      "script": "npm run dev"
    },
    "beforeBuildCommand": {
      "cwd": "../www",
      "script": "npm run build"
    }
  },
  "app": {
    "windows": [
      {
        "title": "OmniKee",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
